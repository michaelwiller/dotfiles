# Default .bash_profile for macOS
# Set PATH to include user binaries and system binaries
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$HOME/bin

# Source .bashrc if it exists
if [ -f ~/.bashrc ]; then
    source ~/.bashrc
fi

[ -f /Users/michael.willer/.ca_bundle_config ] && source /Users/michael.willer/.ca_bundle_config